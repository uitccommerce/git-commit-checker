{
  "name": "uitccommerce/git-commit-checker",
  "description": "Check coding standard & code syntax with Git pre-commit hook.",
  "type": "package",
  "license": "MIT",
  "keywords": [
    "uitccommerce",
    "uitccommerce cms",
    "uitccommerce platform",
    "uitccommerce git commit checker"
  ],
  "homepage": "https://uitccommerce.com",
  "autoload": {
    "psr-4": {
      "Uitccommerce\\GitCommitChecker\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Uitccommerce\\GitCommitChecker\\Tests\\": "tests"
    }
  },
  "require": {
    "php": "^8.0|^8.1|^8.2",
    "laravel/framework": "^9.32|^10.0",
    "laravel/pint": "^1.4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0|^10.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Uitccommerce\\GitCommitChecker\\Providers\\GitCommitCheckerServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable"
}
